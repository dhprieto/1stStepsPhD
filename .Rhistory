max <- max(tabla[, i], na.rm = TRUE)
s <- skew(tabla[, i])
c <- kurtosi(tabla[, i])
#Valores de estadísticos como vector
descriptivos <- as.numeric(c(min, q1, media, media_rec, mediana, moda,

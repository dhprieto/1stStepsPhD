escaladoTablas <- function (datos, varNoEscalar = "noEscalar") {
for (i in colnames(datos)) {
browser()
if (is.numeric(datos[,i]) && i != varNoEscalar){
datos[,i] <- rescale(datos[,i])
}
}
}
